{
    "name": "ngabor84/clapi",
    "description": "Command line tool for API testing",
    "keywords": ["cli", "api", "testing", "clapi"],
    "homepage": "https://github.com/ngabor84/clapi",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor Nemeth",
            "email": "negabor@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "1.0.1",
        "guzzlehttp/guzzle": ">=6.3",
        "ngabor84/guzzle-wsse-middleware": ">=0.1.1",
        "ngabor84/guzzle-escher-middleware": ">=0.1.0",
        "symfony/console": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": ">=3.2.3",
        "kherge/box": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Clapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clapi\\Test\\": "test/"
        }
    }
}
